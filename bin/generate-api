#!/usr/bin/env node

/**
 * Annotate classes.
 */
var API  = require('./helpers/api');

/**
 * Defaults.
 */
var documentationEntry = {
    _name: 'Documentation',
    _path: '/../'
};

var frameworkEntry = {
    _name: 'Framework',
    _path: '/'
};

/**
 * Generate API documentation.
 */
var api = new API({
    breadcrumbs: [ documentationEntry, frameworkEntry ],
    debug: {
        enabled: false,
        exit: false,
        minimalStructure: false,
        dumpCodeFiles: false,
        dumpStructure: false,
        nrOfFiles: 71
    }
}).build();

